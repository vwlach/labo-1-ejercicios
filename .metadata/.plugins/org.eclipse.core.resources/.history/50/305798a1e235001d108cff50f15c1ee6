#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	/*
	int num1;
	int num2;

	printf("Ingrese el primer numero");
	scanf("%d", &num1);

	printf("Ingrese el segundo numero");
	scanf("%d", &num2);
	*/
	int nota;

	do
	{
		printf("Ingrese una nota del 1 al 10: ");
	}while(nota < 0 || nota > 10);

	printf("La nota ingresada es %d", nota);


	return EXIT_SUCCESS;
}
