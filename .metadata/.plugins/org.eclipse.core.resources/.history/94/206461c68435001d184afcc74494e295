#include <stdio.h>
#include <stdlib.h>

char muestroMenu()
{
	char letra;


		do{
					printf(	"Menu\n"
							"a-Iniciar\n"
							"b-Procesar\n"
							"c-Finalizar\n"
							"d-Salir\n");

					printf("\nIngrese una opcion:");
								fflush(stdin);
								scanf("%c", &letra);
			}while (letra != 's');

		return letra ;
}


